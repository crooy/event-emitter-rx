{
  "name": "eventemitter-rx",
  "version": "0.0.2",
  "description": "EventEmitter based on RxJS",
  "main": "src/EventEmitterRx.js",
  "scripts": {
    "start": "npm run tsAMD",
    "tsAMD": "./node_modules/.bin/tsc --t ES5 --m amd --removeComments --outDir dist/ src/EventEmitterRx.ts",
    "tsCommonJS": "./node_modules/.bin/tsc --t ES5 --m commonjs --outDir src/ src/EventEmitterRx.ts",
    "tsTestCommonJS": "./node_modules/.bin/tsc --t ES5 --m commonjs test/EventEmitterRx.spec.ts",
    "mocha": "./node_modules/.bin/mocha test/EventEmitterRx.spec.js",
    "test": "npm run tsCommonJS && npm run tsTestCommonJS && npm run mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/stylelounge/eventemitterrx.git"
  },
  "keywords": [
    "EventEmitter",
    "RxJS"
  ],
  "author": "Jens Krause",
  "license": "MIT",
  "homepage": "https://bitbucket.org/stylelounge/eventemitterrx#readme",
  "dependencies": {
    "rx": "^3.1.0"
  },
  "devDependencies": {
    "chai": "^3.2.0",
    "mocha": "^2.2.5",
    "sinon": "^1.16.1",
    "tsd": "^0.6.3",
    "typescript": "^1.5.3"
  },
  "jspm": {
    "main": "EventEmitterRx",
    "format": "amd",
    "directories": {
      "lib": "dist"
    },
    "dependencies": {
      "rx": "npm:rx@^3.1.0"
    }
  }
}
